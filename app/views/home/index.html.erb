<% if user_signed_in? %>
  <%= link_to "Show All Projects", 'projects' %>
  <%= link_to "Add Project", new_project_path, method: :get %>
  <%= link_to "Log Out", destroy_user_session_path, method: :delete %>
<% else %>
  <div class="row justifty-content-center text-center align-items-center" style="height: 100vh">
    <h1>
      Hello There! Please Click Here to
      <%= link_to "Sign Up", "users/sign_up" %>
    </h1>
  </div>
<% end %>
